{
    "name": "ivan1911/aws-sdk-php-laravel-freezed",
    "homepage": "https://github.com/ivan1911/aws-sdk-php-laravel",
    "description": "A simple Laravel 5 service provider for including the AWS SDK for PHP with fixed AWS library version.",
    "keywords": ["laravel", "laravel 5", "aws", "amazon", "sdk", "s3", "ec2", "dynamodb"],
    "type":"library",
    "license":"Apache-2.0",
    "authors":[
        {
            "name":"Amazon Web Services",
            "homepage":"http://aws.amazon.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "aws/aws-sdk-php": "3.28.4",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "suggest": {
        "laravel/framework": "To test the Laravel bindings",
        "laravel/lumen-framework": "To test the Lumen bindings"
    },
    "autoload": {
        "psr-4": { "Aws\\Laravel\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Aws\\Laravel\\Test\\": "tests/" }
    }
}
